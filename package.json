    "node": ">=16.0.0"
  },
  "main": "./dist/index.js",
  "browser": "./src/index.ts",
  "types": "./src/index.ts",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "develop": "nodemon --ext ts,json --watch src --watch .env --exec 'node --inspect=0.0.0.0:9229' -r dotenv/config -r ts-node/register src/index.ts",
