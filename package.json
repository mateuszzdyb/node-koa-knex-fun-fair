    "knex": "^2.5.1",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-jwt": "^4.0.4",
    "moment": "^2.29.4",
    "objection": "^3.1.1",
    "pg": "^8.11.1",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
